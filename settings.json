  "vsicons.dontShowNewVersionMessage": true,
  "oracledevtools.bookmarkFileFolder": "C:\\Users\\shubh\\Oracle\\oracle.oracledevtools",
  "oracledevtools.download.otherFolder": "C:\\Users\\shubh\\downloads",
  "oracledevtools.connectionConfiguration.configFilesFolder": "C:\\Users\\shubh\\Oracle\\network\\admin",
